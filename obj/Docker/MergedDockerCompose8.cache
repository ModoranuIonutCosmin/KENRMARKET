services:
  cart.api:
    build:
      context: C:\Users\zero6\source\repos\KMarketplace
      dockerfile: src/Cart/Cart.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Cart.API
      target: base
    container_name: Cart.API
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ConnectionStrings__SqlServer: Server=host.docker.internal\\MSSQLLocalDB;User
        ID=local;Password=aaaaAAAA1234;Initial Catalog=Carts
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: cartapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/Cart.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 6002
      target: 80
    tty: true
    volumes:
    - C:\Users\zero6\source\repos\KMarketplace\src\Cart\Cart.API:/app:rw
    - C:\Users\zero6\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\zero6\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
